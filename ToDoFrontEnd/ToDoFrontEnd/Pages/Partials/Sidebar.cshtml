@using ToDoFrontEnd.Services.Dtos
@model List<TitleDto>
@{
}
<div class="list-group list-group-flush md-4">
    @if (Model != null)
    {
        @foreach (var item in Model)
        {
            <a class="list-group-item list-group-item-action list-group-item-light p-3" method="get" asp-route-titleId="@item.Id" asp-page-handler="OnClickTitle">
                @item.Title
            </a>
        }
    }
</div>
